#footer

- if ( user != undefined )

  div( class = 'row' )

    div( class = 'col-lg-4' )

      div( class = 'list-group', style = 'opacity: 1; height: 128px' )

        - var stringy = ''; 
        - var counter = 12;
        - var cap = ','

        - var random = function ( a, b ) { return Math.floor((Math.random()*b)+a) }

        - for ( var i = 0; i < counter; i++ ) {
        -   if ( i == counter - 1 ) cap = ''
        -   stringy += ( random( 5, 8 ) ) + cap;
        - };

        span( class = 'cela-activity' ) #{ stringy }

    div( class = 'col-lg-4' )

      div( class = 'list-group' )

        a( href = '/admin/users', class = 'list-group-item' )
          h4( class = 'list-group-item-heading' ) List Users
          p( class = 'list-group-item-text' ) User Management

        a( href = '/admin/users/new', class = 'list-group-item' )
          h4( class = 'list-group-item-heading' ) Create Users
          p( class = 'list-group-item-text' ) User Management

    div( class = 'col-lg-4' )

      div( class = 'list-group' )

        a( href = '/account', class = 'list-group-item' )
          h4( class = 'list-group-item-heading' ) Account
          p( class = 'list-group-item-text' ) Manage your account

        a( href = '/account/logout', class = 'list-group-item' )
          h4( class = 'list-group-item-heading' ) Logout
          p( class = 'list-group-item-text' ) Sign out of your account

  - // div()

    - // div #{ title } by 

      - // a( href = 'http://twitter.com/jasonhargrove', target = '_blank', style = 'color: #27ae60' ) Jason Hargrove

  div( class = 'navbar navbar-fixed-bottom', style = 'background-color: #7f8c8d; display: none' )

script( type='text/javascript', src='/moda/common_views/assets/js/jquery.js' )

script( type='text/javascript', src='/moda/common_views/assets/js/bootstrap.js' )

script( type='text/javascript', src='/moda/common_views/assets/js/jquery.peity.js' )

script.


  $('.cela-activity').peity('line', {

    colour: '#EEEEEE',

    strokeColour: '#3A87AD',

    strokeWidth: 2,

    width: '100%', height: '100%'

  })




